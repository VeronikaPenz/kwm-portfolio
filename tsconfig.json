{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets": [
        "./"
      ]
    },
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src",
    "cypress"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
